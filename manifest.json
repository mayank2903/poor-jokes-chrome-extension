{
  "manifest_version": 3,
  "name": "Joor Pokes - Poor Jokes In Every New Tab",
  "version": "1.0.0",
  "description": "Transform your new tab into a laughter-filled experience with poor jokes, ratings, and community submissions!",
  "short_name": "Joor Pokes",
  "author": "Poor Jokes Team",
  "homepage_url": "https://poor-jokes-newtab.vercel.app",
  "permissions": [],
  "host_permissions": [
    "https://poor-jokes-newtab.vercel.app/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["styles.css", "js/*", "icons/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "minimum_chrome_version": "88"
}